{
  "publisher": "barbapapazes",
  "name": "utils-snippets-vscode",
  "displayName": "Utils Snippets",
  "type": "module",
  "version": "0.1.1",
  "private": true,
  "packageManager": "pnpm@10.9.0",
  "description": "Become a 10x engineer with snippets",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/barbapapazes/utils-snippets"
  },
  "categories": [
    "Snippets"
  ],
  "engines": {
    "vscode": "^1.102.0"
  },
  "contributes": {
    "snippets": [
      {
        "language": "",
        "path": "./snippets/snippets.code-snippets"
      },
      {
        "language": "markdown",
        "path": "./snippets/md.json"
      },
      {
        "language": "vue",
        "path": "./snippets/vue.json"
      },
      {
        "language": "typescript",
        "path": "./snippets/typescript.json"
      },
      {
        "language": "php",
        "path": "./snippets/php.json"
      },
      {
        "language": "html",
        "path": "./snippets/html.json"
      }
    ]
  },
  "scripts": {
    "publish": "vsce publish --no-dependencies",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "release": "npm run lint && bumpp"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.12.0",
    "@vscode/vsce": "^3.3.2",
    "bumpp": "^10.1.0",
    "eslint": "^9.25.1"
  }
}
